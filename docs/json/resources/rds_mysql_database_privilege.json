{
  "provider_schemas": {
    "huaweicloud/huaweicloud": {
      "resource_schemas": {
        "huaweicloud_rds_mysql_database_privilege": {
          "block": {
            "attributes": {
              "db_name": {
                "type": "string",
                "description": "The database name to which the users (accounts) are privileged.",
                "required": true
              },
              "id": {
                "type": "string",
                "computed": true
              },
              "instance_id": {
                "type": "string",
                "description": "The ID of the MySQL instance.",
                "required": true
              },
              "region": {
                "type": "string",
                "description": "The region where the database and users (accounts) are located.",
                "computed": true
              }
            },
            "block_types": {
              "users": {
                "block": {
                  "attributes": {
                    "name": {
                      "type": "string",
                      "description": "The username of the database account.",
                      "required": true
                    },
                    "readonly": {
                      "type": "bool",
                      "description": "Whether the user has read-only permission.",
                      "optional": true,
                      "computed": true
                    }
                  },
                  "description": "The user (account) permissions with the database."
                },
                "nesting_mode": "list",
                "min_items": 1
              },
              "users_origin": {
                "block": {
                  "attributes": {
                    "name": {
                      "type": "string",
                      "description": "Specifies the username of the database account.",
                      "computed": true
                    },
                    "readonly": {
                      "type": "bool",
                      "description": "Specifies the read-only permission.",
                      "computed": true
                    }
                  },
                  "description": "schema:{\"Internal\":true};The script configuration value of this change is also the original value used for comparison with\nthe new value next time the change is made. The corresponding parameter name is 'users'."
                },
                "nesting_mode": "list"
              }
            }
          },
          "product": {
            "short": "RDS",
            "name": "Relational Database Service",
            "catalog": "Databases",
            "name_cn": "云数据库",
            "catalog_cn": "数据库"
          }
        }
      }
    }
  }
}
