{
  "provider_schemas": {
    "huaweicloud/huaweicloud": {
      "resource_schemas": {
        "huaweicloud_identity_provider_conversion": {
          "block": {
            "attributes": {
              "id": {
                "type": "string",
                "computed": true
              },
              "provider_id": {
                "type": "string",
                "description": "The ID of the identity provider used to manage the conversion rules.",
                "required": true,
                "forcenew": true
              }
            },
            "block_types": {
              "conversion_rules": {
                "block": {
                  "block_types": {
                    "local": {
                      "block": {
                        "attributes": {
                          "group": {
                            "type": "string",
                            "description": "The user group to which the federated user belongs on the\n                                               cloud platform.",
                            "optional": true
                          },
                          "username": {
                            "type": "string",
                            "description": "The name of a federated user on the cloud platform.",
                            "optional": true
                          }
                        },
                        "description": "The federated user information on the cloud platform."
                      },
                      "nesting_mode": "list",
                      "min_items": 1
                    },
                    "remote": {
                      "block": {
                        "attributes": {
                          "attribute": {
                            "type": "string",
                            "description": "The attribute in the IDP assertion.",
                            "required": true
                          },
                          "condition": {
                            "type": "string",
                            "description": "The condition of conversion rule.",
                            "optional": true
                          },
                          "value": {
                            "type": [
                              "list",
                              "string"
                            ],
                            "description": "The rule is matched only if the specified strings appear in the\n                                            attribute type.",
                            "optional": true
                          }
                        },
                        "description": "Federated user information in the IDP system."
                      },
                      "nesting_mode": "list",
                      "min_items": 1
                    }
                  },
                  "description": "The identity conversion rules of the identity provider."
                },
                "nesting_mode": "list",
                "min_items": 1
              }
            }
          },
          "product": {
            "short": "IAM",
            "name": "Identity and Access Management",
            "catalog": "Management & Governance",
            "name_cn": "统一身份认证服务",
            "catalog_cn": "管理与监管"
          }
        }
      }
    }
  }
}
