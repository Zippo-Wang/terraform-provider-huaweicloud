                "required": true
              },
              "provider_id": {
                "type": "string",
                "description": "The ID of the identity provider used to manage the protocol.",
                "required": true
              }
            }
          },
          "product": {
            "short": "IAM",
            "name": "Identity and Access Management",
            "catalog": "Management & Governance",
