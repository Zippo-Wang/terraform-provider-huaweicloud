          "block": {
            "attributes": {
              "id": {
                "type": "string",
                "computed": true
              },
              "links": {
                "type": [
                  "list",
                  [
                    "object",
                    {
                      "identity_provider": "string",
                      "self": "string"
                    }
                  ]
                ],
                "description": "The links of protocol.",
                "computed": true
              },
              "mapping_id": {
                "type": "string",
                "description": "The mapping_id for the protocol.",
                "optional": true
              },
              "protocol_id": {
                "type": "string",
